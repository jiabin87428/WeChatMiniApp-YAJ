<!--pages/check/firstCheck.wxml-->
<view class="baseView">
  <scroll-view scroll-y="true" class='scrollView'>
    <view class='scrollItem' bindtap='jumpBaseInfo'>
      <text class='cellText1'>企业基本信息</text>
      <text class='mustText'>*</text>
      <text class='cellText2'></text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view class='scrollItem' bindtap='jumpSafety'>
      <text class='cellText1'>安全管理信息</text>
      <text class='mustText'>*</text>
      <text class='cellText2'></text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view class='scrollItem' bindtap='jumpDevice'>
      <text class='cellText1'>设备信息</text>
      <text class='cellText2'></text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view class='scrollItem' bindtap='jumpDangerList'>
      <text class='cellText1'>隐患信息</text>
      <text class='cellText2'></text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view class='scrollItem'>
      <text class='cellText1'>是否需复查</text>
      <text class='cellText2'></text>
      <switch checked="{{needReCheck}}" bindchange="switch2Change"/>
    </view>
    <view id='clqk' class='scrollItem' bindtap='jumpInput'>
      <text class='cellText1'>处理情况</text>
      <text class='cellText2'>{{clqk}}</text>
      <image class="arrow" src="../../assets/arrowRight.png" mode="aspectFit"></image>
    </view>
    <view id='9' class='scrollButtonItem'>
      <button class='submitButton' bindtap="submitClick">提交</button>
    </view>
  </scroll-view>
</view>
